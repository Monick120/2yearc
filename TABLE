int a;
    printf("enter a number");
    scanf("%d",&a);
    if(a%2==0)
     {
         for(int i=1;i<10;++i)
         {
             printf("\n%d * %d= %d",i,a,i*a);
             
         }
     } 
     else{
         for(int i=2;i<15;i++)
         {
             printf("\n%d * %d=%d",i,a,i*a);
             i=i+1;
         }
     }
}
